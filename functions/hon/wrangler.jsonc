{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "hon",
	"main": "src/index.ts",
	"compatibility_date": "2025-03-11",
	"observability": {
		"enabled": true,
		"head_sampling_rate": 1
	},
	"vars": {
		"ENVIRONMENT": "local"
	},
	"env": {
		"staging": {
			"vars": {
				"ENVIRONMENT": "staging"
			}
		},
		"production": {
			"vars": {
				"ENVIRONMENT": "production"
			}
		}
	}
}
